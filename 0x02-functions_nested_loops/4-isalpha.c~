#include "main.h"
/**
 * _isalpha - check if charachter is alpha
 * @c: The charachter to check
 *
 * Return: 1 if islower case 0 if not.
 * On error, -1 is returned, and errno is set appropriately.
 */
int _isalpha(int c)
{
	if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))
	{
		return (1);
	}

	return (0);
}
